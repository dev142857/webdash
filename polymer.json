{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/web-dash.html",
    "src/view-404.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "utils/*.js",
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
  {
    "name": "default",
    "bundle": false,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }
  ]
}
