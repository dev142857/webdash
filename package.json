{
  "name": "webdash",
  "version": "0.0.20",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./scripts/install.js",
    "dev": "concurrently \"polymer serve\" \"node serve.js\"",
    "start": "concurrently \"polymer serve build/es6-bundled\" \"node api.js\"",
    "prepublishOnly": "polymer build",
    "lint": "npm run lint:javascript && polymer lint",
    "lint:javascript": "eslint . --ext js,html --ignore-path .gitignore",
    "test": "polymer test",
    "test:integration": "polymer build"
  },
  "files": [
    "*"
  ],
  "bin": {
    "webdash": "./scripts/index.js"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "chalk": "^2.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "pkg-dir": "^2.0.0",
    "serve-static": "^1.13.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^9.4.0",
    "concurrently": "^3.5.1",
    "eslint": "^3.19.0",
    "eslint-plugin-html": "^2.0.0"
  }
}
